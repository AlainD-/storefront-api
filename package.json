{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "Node project API for a shopping application",
  "main": "src/index.ts",
  "scripts": {
    "prestart": "npm run migrate:dev:up",
    "start": "nodemon src/index.ts",
    "build": "tsc --project ./",
    "lint": "eslint \"src/**/*.ts\"",
    "jasmine": "jasmine",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "pretest": "npm run migrate:test:up",
    "test": "npm run build && npm run jasmine",
    "posttest": "npm run migrate:test:down",
    "migrate:dev:up": "db-migrate up -e dev",
    "migrate:dev:down": "db-migrate down -e dev",
    "migrate:test:up": "db-migrate up -e test",
    "migrate:test:down": "db-migrate down -e test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlainD-/storefront-api.git"
  },
  "keywords": [
    "node",
    "express",
    "javascript",
    "typescript",
    "api",
    "udacity"
  ],
  "author": "Alain D'EURVEILHER <alain.deurveilher@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlainD-/storefront-api/issues"
  },
  "homepage": "https://github.com/AlainD-/storefront-api#readme",
  "dependencies": {
    "cors": "2.8.5",
    "db-migrate-pg": "1.2.2",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "helmet": "4.6.0",
    "jasmine": "3.10.0",
    "jasmine-spec-reporter": "7.0.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "pg": "8.7.1",
    "supertest": "6.1.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/jasmine": "3.10.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.7",
    "@types/pg": "8.6.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": "16.5.0",
    "npm": "7.19.1"
  }
}
